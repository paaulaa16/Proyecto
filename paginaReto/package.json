{
    "name": "paginaReto",
    "version": "1.0.0",
    "license": "MIT",
    "private": true,
    "scripts": {
        "build": "parcel build *.html --public-url /2daw3/paulac/pagina",
        "proxy": "parcel build *.html",
        "prebuild": "(test -d dist || mkdir dist) && cp -rv imagenes dist",
        "preproxy": "yarn prebuild",
        "prestart": "yarn prebuild",
        "start": "parcel serve *.html --open"
    },
    "devDependencies": {
        "@parcel/transformer-sass": "^2.0.1",
        "parcel": "^2.0.1"
    },
    "dependencies": {
        "bootstrap": "^5.1.3",
        "leaflet": "^1.7.1",
        "sass": "^1.45.1",
        "yarn": "^1.22.17"
    },
    "alias": {
        "leaflet": {
            "global": "L"
        },
        "bootstrap": {
            "global": "bootstrap"
        }
    }
}